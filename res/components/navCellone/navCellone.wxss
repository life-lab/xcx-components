/* components/navCellOne/navCellone.wxss */
.navCell {
  width: 100%;
}

.navCellItem {
  margin-left: 16px;
  border-bottom: 0.1px solid #eeeeee;
  display: flex;
  flex: row;
}

.navCellItem view {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center; 
}

.navCellItem image {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center; 
}

.navCellItem .image-left{
  width: 40px;
  height: 40px;
}

.navCellItem .image-right{
  width: 10px;
}

.navCellItem .navContent {
  align-items: flex-start;
  margin: 0 16px;
}

.navCellItem .navContent .text-one {
  font-size: 16px;
  color: #2b2b2b;
}

.navCellItem .navContent .text-two {
  font-size: 12px;
  padding-top: 3px;
  color: #6b6b6b;
}
